    <TextView
        android:id="@+id/tv_firmware_protect_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/label_value_spacing"
        android:layout_marginEnd="@dimen/label_value_spacing"
        android:text="@string/firmware_protect_value_unknown"
        android:textIsSelectable="true"
        app:layout_constraintBottom_toBottomOf="@id/tv_firmware_protect"
        app:layout_constraintStart_toEndOf="@id/tv_firmware_protect"
        app:layout_constraintTop_toTopOf="@id/tv_firmware_protect" />
    <View
        android:id="@+id/divider5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/label_vertical_spacing"
        android:background="?android:attr/dividerVertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_firmware_protect" />

    <TextView
        android:id="@+id/tv_firmware_structLen"
