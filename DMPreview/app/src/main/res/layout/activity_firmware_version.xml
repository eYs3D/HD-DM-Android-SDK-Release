        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider3" />

    <TextView
        android:id="@+id/tv_serial_number_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/label_value_spacing"
        android:layout_marginEnd="@dimen/label_value_spacing"
        android:text="@string/firmware_version_value_unknown"
        android:textIsSelectable="true"
        app:layout_constraintBottom_toBottomOf="@id/tv_serial_number"
        app:layout_constraintStart_toEndOf="@id/tv_serial_number"
        app:layout_constraintTop_toTopOf="@id/tv_serial_number" />

    <View
        android:id="@+id/divider4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/label_vertical_spacing"
        android:background="?android:attr/dividerVertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_serial_number" />

    <TextView
        android:id="@+id/tv_firmware_protect"
        style="@style/common_settings_label"
