        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_vendor_id" />

    <TextView
        android:id="@+id/tv_serial_number"
        style="@style/common_settings_label"
        android:text="@string/firmware_version_serial_number"
