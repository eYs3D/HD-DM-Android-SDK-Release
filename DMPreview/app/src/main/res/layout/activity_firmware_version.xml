        android:layout_marginTop="@dimen/label_vertical_spacing"
        android:background="?android:attr/dividerVertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_firmware_unpAreaStartSec" />

    <Button
        android:id="@+id/tv_read3x"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:enabled="false"
        android:text="@string/firmware_read3X"
        app:layout_constraintRight_toRightOf="@+id/tv_write3x"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider7" />

    <Button
        android:id="@+id/tv_write3x"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
